<view class="evaluation-table">
    <view class="tr bg-w">
        <view class="th">状态</view>
        <view class="th">课程名称</view>
        <view class="th ">课程类型</view>
        <view class="th ">教师名称</view>
    </view>
    <block wx:for="{{listData}}" wx:key="{{code}}">
        <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
            <view class="td">{{item.state}}</view>
            <view class="td">{{item.course_name}}</view>
            <view class="td">{{item.course_type}}</view>
            <view class="td">{{item.teacher}}</view>
        </view>
        <view class="tr" wx:else>
            <view class="td">{{item.state}}</view>
            <view class="td">{{item.course_name}}</view>
            <view class="td">{{item.course_type}}</view>
            <view class="td">{{item.teacher}}</view>
        </view>
    </block>
</view>