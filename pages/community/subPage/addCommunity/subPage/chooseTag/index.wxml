<view class='m-backgraund'>
  <button bindtap='chooseFriend' wx:for='{{ friend }}' wx:key='index' class='friend' data-pos='{{ index }}'>
    <view class='click'>
      <image wx:if='{{ item.choose }}' src='/images/icon/choose.png' class='icon'></image>
    </view>
    <image class='head'></image>
    <view class='name'>{{ item.name }}</view>
  </button>

  <button bindtap='submit'class='friend submit'>
    <view>提交</view>
  </button>
</view>