page {
  display: flex;
  flex-direction: column;
  flex: 1;
  min-height: 100%;
}

.m-backgraund {
  background-image: url('https://s2.ax1x.com/2019/02/28/k7Ub9S.png');
  background-size: cover;
  background-attachment: fixed;
  flex: 1;
}

.m-backgraund .input{
  background-color: #f9f9f9;
  width: 590rpx;
  margin: 20rpx 40rpx 0rpx 40rpx;
  padding: 10rpx 40rpx 10rpx 40rpx;
  border-radius: 10rpx;
}

.placeholder {
  font-size: 35rpx;
  color: #ccc;
}
button {
  background: transparent;
  margin-top: 22rpx;
  padding: 0;
  width: 683rpx;
  height: 78rpx;
}
image {
  width: 683rpx;
  height: 78rpx;
}

